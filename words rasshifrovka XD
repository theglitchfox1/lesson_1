words = {
        "ЛОЛ": "Что-то очень смешное",
        "КРИНЖ": "Что-то очень странное или стыдное",
        "РОФЛ": "шутка",
        "ЩИЩ": "одобрение или восторг",
        "КРИПОВЫЙ": "страшный,пугающий",
        "АГРИТЬСЯ": "злиться"
        }
        
while True:
    word = input('Введите ваше слово (ЗГЛАВНЫМИ БУКВАМИ!)')

    if word in words.keys():
        print(words[word])
    else:
        print('Это слово еще не внесено в нашу базу,приносим сввои извинения(((')
